                  <ChoiceBox fx:id="targetsChoiceBox" maxHeight="50.0" maxWidth="1.7976931348623157E308" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="3" GridPane.rowSpan="2" />
