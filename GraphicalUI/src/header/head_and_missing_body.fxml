<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>

<BorderPane fx:id="base_BorderPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="header.HeaderController">
   <top>
      <GridPane>
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
         <columnConstraints>
            <ColumnConstraints hgrow="NEVER" maxWidth="30.0" minWidth="30.0" prefWidth="30.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
            <ColumnConstraints hgrow="NEVER" maxWidth="30.0" minWidth="30.0" prefWidth="30.0" />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints vgrow="NEVER" />
         </rowConstraints>
         <children>
            <HBox alignment="CENTER_LEFT" fillHeight="false" prefHeight="50.0" spacing="10.0" GridPane.columnIndex="1">
               <children>
                  <Button fx:id="graphInfo_btn" disable="true" mnemonicParsing="false" onAction="#loadGraphInfo" text="Graph Info" />
                  <Button fx:id="findPath_btn" disable="true" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#loadFindPath" text="Find Path" />
                  <Button fx:id="findCycle_btn" disable="true" layoutX="62.0" layoutY="10.0" mnemonicParsing="false" onAction="#loadFindCycle" text="Find Cycle" />
                  <Button fx:id="whatIf_btn" disable="true" layoutX="114.0" layoutY="10.0" mnemonicParsing="false" onAction="#loadWhatIf" text="What if" />
                  <Button fx:id="runTask_btn" disable="true" layoutX="166.0" layoutY="10.0" mnemonicParsing="false" onAction="#loadTaskRun" text="Run Task" />
                  <Button fx:id="uploadFile_btn" layoutX="218.0" layoutY="10.0" mnemonicParsing="false" onAction="#loadFileLoader" text="Upload File" />
               </children>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
            </HBox>
         </children>
      </GridPane>
   </top>
</BorderPane>
