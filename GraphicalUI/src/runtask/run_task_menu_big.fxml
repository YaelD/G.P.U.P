<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.HBox?>


<HBox alignment="CENTER" minHeight="-Infinity" minWidth="-Infinity" spacing="10.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="runtask.RunTaskMenuController">
   <children>
      <ScrollPane fitToHeight="true" fitToWidth="true">
         <content>
            <fx:include fx:id="chooseTargetsToggles" source="run_task_choose_targets.fxml" />
         </content>
      </ScrollPane>
      <ScrollPane fitToHeight="true" fitToWidth="true">
         <content>
            <fx:include fx:id="chooseThreadsAndTaskToggles" source="run_task_toggles_choose_num_threads_and_tasks.fxml" />
         </content>
      </ScrollPane>
      <ScrollPane fitToHeight="true" fitToWidth="true" visible="false">
         <content>
            <fx:include fx:id="simulationTaskToggles" source="simlation_task_toggles.fxml" />
         </content>
      </ScrollPane>
      <ScrollPane fitToHeight="true" fitToWidth="true" visible="false">
         <content>
            <fx:include fx:id="compilationTaskToggles" source="compilation_task_toggles.fxml" />
         </content>
      </ScrollPane>
   </children>
</HBox>
