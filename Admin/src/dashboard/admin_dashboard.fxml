<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dashboard.DashboardController">
   <center>
      <VBox fillWidth="false" spacing="10.0" BorderPane.alignment="CENTER">
         <children>
            <HBox alignment="CENTER" layoutX="10.0" layoutY="10.0" spacing="60.0">
               <children>
                  <Label maxHeight="1.7976931348623157E308" text="System Graphs" underline="true">
                     <font>
                        <Font size="21.0" />
                     </font>
                  </Label>
                  <Button mnemonicParsing="false" onAction="#onUploadGraphClick" prefHeight="27.0" prefWidth="134.0" text="Upload new Graph" />
               </children>
            </HBox>
            <ScrollPane>
               <content>
                  <fx:include fx:id="graphsTable" source="graphs_table.fxml" />
               </content>
            </ScrollPane>
            <HBox alignment="CENTER" spacing="60.0">
               <children>
                  <Label maxHeight="1.7976931348623157E308" text="System Tasks" underline="true">
                     <font>
                        <Font size="21.0" />
                     </font>
                  </Label>
                  <Button fx:id="taskDetailsButton" mnemonicParsing="false" onAction="#onClickTaskDetails" prefHeight="27.0" prefWidth="134.0" text="Task Details" />
               </children>
            </HBox>
            <ScrollPane>
               <content>
                  <fx:include fx:id="tasksTable" source="task_table.fxml" />
               </content>
            </ScrollPane>
         </children>
      </VBox>
   </center>
   <left>
      <fx:include fx:id="usersList" source="users_list.fxml" />
   </left>
   <top>
      <AnchorPane prefHeight="20.0" BorderPane.alignment="CENTER" />
   </top>
</BorderPane>
